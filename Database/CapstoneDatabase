DROP TABLE IF EXISTS `Freelancer`;
DROP TABLE IF EXISTS `WebServices`;

CREATE TABLE `Freelancer` (
  `f_id` int(11) NOT NULL,
  `f_name` varchar(45) NOT NULL,
  `f_email` varchar(45) NOT NULL,
  `f_website` varchar(100) DEFAULT NULL,
  PRIMARY KEY (`f_id`)
)

CREATE TABLE `WebServices` (
  `w_name` varchar(100) NOT NULL,
  `domain` varchar(255) NOT NULL,
  `feature1` varchar(255) NOT NULL,
  `feature2` varchar(255) DEFAULT NULL,
  `feature3` varchar(255) DEFAULT NULL,
  `w_id` varchar(2) NOT NULL,
  PRIMARY KEY (`w_id`)
)


